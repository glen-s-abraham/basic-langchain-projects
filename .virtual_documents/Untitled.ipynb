


import os
from dotenv import load_dotenv,find_dotenv
load_dotenv(find_dotenv())


from langchain.llms import OpenAI


llm = OpenAI(model='text-davinci-003')


query = 'What is the currency of india?'
completion = llm(query)
print(completion)





